  var canvas;
  canvas = document.getElementById("glcanvas");
  var gl = canvas.getContext("experimental-webgl");

  //console.log(gl.getParameter(gl.RENDERER));
  //console.log(gl.getParameter(gl.VENDOR));
  //console.log(getUnmaskedInfo(gl).vendor);
  //console.log(getUnmaskedInfo(gl).renderer);

  var googleGpu = getUnmaskedInfo(gl).renderer;

  function getUnmaskedInfo(gl) {
    var unMaskedInfo = {
      renderer: '',
      vendor: ''
    };

    var dbgRenderInfo = gl.getExtension("WEBGL_debug_renderer_info");
    if (dbgRenderInfo != null) {
      unMaskedInfo.renderer = gl.getParameter(dbgRenderInfo.UNMASKED_RENDERER_WEBGL);
      unMaskedInfo.vendor = gl.getParameter(dbgRenderInfo.UNMASKED_VENDOR_WEBGL);
    }

    return unMaskedInfo;
  }

  if (googleGpu.includes("SwiftShader") === true) {
    console.log("gpu deactivated");

  } else {
    console.log("gpu activated");

    const gpuSelector = document.querySelectorAll('.gpu_disable');

    gpuSelector.forEach(gpu_disable => {
      gpu_disable.classList.remove('gpu_disable');
    });

  };

